{
  "name": "console.freelog.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "freelog-ci deploy",
    "dev": "lerna link && webpack-dev-server --config ./config/webpack.dev.js",
    "build": "rm -rf ./dist/** && webpack --config ./config/webpack.prod.js",
    "build:analyze": "webpack --config ./config/webpack.analyze.js",
    "build:prod": "npm run build -- --env=prod",
    "build:beta": "npm run build -- --env=beta",
    "build:test": "npm run build -- --env=test",
    "pack": "tar -zcf console.tar dist",
    "test:e2e": "vue-cli-service test:e2e --config ./nightwatch.conf.js",
    "test:e2e:chrome": "npm run test:e2e -- --env chrome",
    "test:e2e:safari": "npm run test:e2e -- --env safari",
    "test:e2e:firefox": "npm run test:e2e -- --env firefox",
    "test:e2e:all": "npm run test:e2e -- --env chrome,firefox,safari"
  },
  "dependencies": {

    "codemirror": "^5.42.2",
    "core-js": "^2.6.5",
    "cropperjs": "^1.5.1",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.30.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.10",
    "lozad": "^1.7.0",
    "nprogress": "^0.2.0",
    "object-path": "^0.11.4",
    "querystring": "^0.2.0",
    "quill": "^1.3.6",
    "screenfull": "^3.3.3",
    "semver": "^6.3.0",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vue-lazyload": "^1.2.6",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@freelog/freelog-ci": "0.0.2",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/babel-preset-app": "^3.8.0",
    "ali-oss": "^6.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "co": "^4.6.0",
    "consola": "^1.4.5",
    "css-loader": "^2.1.1",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-json": "^1.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "geckodriver": "^1.14.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "preload-webpack-plugin": "^2.3.0",
    "selenium-server": "^3.141.59",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "user-home": "^2.0.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "walk": "^2.3.14",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "freelog-ci deploy"
    }
  }
}
