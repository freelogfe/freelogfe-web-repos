{
  "name": "user",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.7",
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.10.4",
    "@freelog/components-lib": "^0.1.22",
    "@freelog/tools-lib": "^0.1.138",
    "ahooks": "^3.4.1",
    "animate.css": "^4.1.1",
    "cropperjs": "^1.5.12",
    "dva": "^2.4.1",
    "react": "^18.2.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^18.2.0",
    "redux": "^4.2.0",
    "umi": "^3.4.21"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@umijs/preset-react": "^1.8.8",
    "@umijs/test": "^3.4.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.7.4"
  }
}
