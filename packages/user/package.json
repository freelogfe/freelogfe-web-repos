{
  "name": "user",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.7",
    "ahooks": "^3.4.1",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^18.2.0",
    "react-monaco-editor": "^0.46.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/semver": "^7.3.9",
    "@umijs/preset-react": "^1.8.8",
    "@umijs/test": "^3.4.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "umi": "^3.4.21"
  }
}
